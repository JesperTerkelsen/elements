  },
  "publishConfig": {
    "access": "public"
  },
  "storybook-deployer": {
    "gitUsername": "Tradeshift Elements",
    "gitEmail": "lza@tradeshift.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  }
}
